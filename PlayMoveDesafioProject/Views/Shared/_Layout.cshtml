<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PlayMove</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;700&display=swap" rel="stylesheet">

    <link rel="icon" type="image/x-icon" href="~/Cork/Images/playmove.jpg" />

    <link href="~/Cork/demo3/assets/css/loader.css" rel="stylesheet" type="text/css" />
    <script src="~/Cork/demo3/assets/js/loader.js"></script>

    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="https://fonts.googleapis.com/css?family=Nunito:400,600,700" rel="stylesheet">
    <link href="~/Cork/demo3/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Cork/demo3/assets/css/plugins.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="~/Cork/demo3/assets/css/elements/alert.css">
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM STYLES -->
    <!--  BEGIN CUSTOM STYLE FILE  -->
    <link href="~/Cork/demo3/assets/css/scrollspyNav.css" rel="stylesheet" type="text/css" />
    <link href="~/Cork/demo3/assets/css/components/tabs-accordian/custom-tabs.css" rel="stylesheet" type="text/css" />
    <link href="~/Cork/demo3/plugins/animate/animate.css" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="~/Cork/demo3/plugins/font-icons/fontawesome/css/regular.css">
    <link rel="stylesheet" href="~/Cork/demo3/plugins/font-icons/fontawesome/css/fontawesome.css">
    <script src="https://kit.fontawesome.com/dde268f992.js" crossorigin="anonymous"></script>

    <style>
        body {
            /*font-family: 'Roboto', sans-serif !important;*/
        }

        .far {
            margin-right: 10px;
            vertical-align: middle;
            width: 20px;
            height: 20px;
            font-size: 18px;
            padding-left: 2px;
        }

        .layout-px-spacing {
            min-height: calc(100vh - 166px) !important;
        }


        @@media (min-width: 576px) {
            .container {
                /*max-width: 540px;*/
                max-width: 96% !important;
            }
        }

        @@media (min-width: 768px) {
            .container {
                /*max-width: 720px;*/
                max-width: 96% !important;
            }
        }

        @@media (min-width: 992px) {
            .container {
                max-width: 96% !important;
                /*max-width: 960px;*/
            }
        }

        @@media (min-width: 1200px) {
            .container {
                max-width: 96% !important;
                /*max-width: 1140px;*/
            }
        }

        .modal-header {
            border-bottom: 1px solid #888ea8 !important;
        }

        .modal-footer {
            border-top: 1px solid #888ea8 !important;
        }

        .modal-title {
            color: #888ea8 !important;
            font-size: 16px;
        }

        .close {
            color: #888ea8 !important;
            text-shadow: 0 0px 0 #007be4 !important;
        }

        .goog-te-banner-frame.skiptranslate {
            display: none !important;
        }

        #google_translate_element {
            position: relative;
            z-index: 1030;
            background-color: transparent;
        }

        .goog-te-gadget-simple {
            background-color: transparent !important;
            border: 0px solid #0084ff !important;
            padding: 1px !important;
            border-radius: 2px !important;
            font-size: 1rem !important;
            line-height: 2rem !important;
            display: inline-block;
            cursor: pointer;
            zoom: 1;
            color: #353535 !important;
            font-weight: bold !important;
        }

            .goog-te-gadget-simple img {
                display: none;
            }

        .goog-te-menu2 {
            max-width: 10% !important;
            background-color: #272e48 !important;
        }

        .goog-te-menu2-item div, .goog-te-menu2-item:link div, .goog-te-menu2-item:visited div {
            background-color: #272e48 !important;
        }

        .f-section-2 {
            display: flex;
            justify-content: center;
        }

        .footer-wrapper {
            margin-top: 30px;
        }

        .contact-li {
            display: flex;
            justify-content: space-between;
            font-size: 13px;
            color: #506690;
            padding: 11px 14px;
            transition: .600s;
            margin-bottom: 4px;
            font-weight: 600;
            letter-spacing: 1px;
            align-items: flex-start;
            flex-direction: column;
            align-content: center;
        }

        .footer-wrapper {
            background: #1a1c2d;
        }
    </style>
</head>
<body class="alt-menu">
    @Html.AntiForgeryToken()
    <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="tk" class="modal animated fadeInUp custo-fadeInUp modal-lg modal-dialog-centered" style="display: none; max-width:100% !important">

    </div>

    <!-- BEGIN LOADER -->
    <div id="load_screen">
        <div class="loader">
            <div class="loader-content">
                <div class="spinner-grow align-self-center"></div>
            </div>
        </div>
    </div>
    <!--  END LOADER -->
    <!-- ===== Main-Wrapper ===== -->
    <!-- ===== Top-Navigation ===== -->
    <partial name="TopNav.cshtml" />
    <!-- ===== Top-Navigation-End ===== -->
    <!--  BEGIN MAIN CONTAINER  -->
    <div class="main-container" id="container">

        <div class="overlay"></div>
        <div class="search-overlay"></div>

        <!--  BEGIN SIDEBAR  -->
        <partial name="Menu.cshtml" />
        <!--  END SIDEBAR  -->
        <!--  BEGIN CONTENT PART  -->
        <div id="content" class="main-content">
            <div class="layout-px-spacing">
                <div class="row layout-top-spacing">
                    @RenderBody()

                </div>
            </div>

            <div class="footer-wrapper">
                <div class="footer-section f-section-1">
                </div>
                <div class="footer-section f-section-2">
                    <p class="">Copyright © 2022 <a target="_blank" href="https://www.linkedin.com/company/playmove/?originalSubdomain=br">Desafio PlayMove</a>, All rights reserved.</p>
                </div>

            </div>


        </div>
        <!--  END CONTENT PART  -->
    </div>
    <!-- BEGIN GLOBAL MANDATORY SCRIPTS -->
    <script src="~/Cork/demo3/assets/js/libs/jquery-3.1.1.min.js"></script>
    <script src="~/Cork/demo3/bootstrap/js/popper.min.js"></script>
    <script src="~/Cork/demo3/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Cork/demo3/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Cork/demo3/assets/js/app.js"></script>
    <script src="~/assets/plugins/BOTMONEY/jquery.maskMoney.js"></script>


    <script>
        $(document).ready(function () {
            App.init();
            $('.money').maskMoney({ prefix: '$ ', allowNegative: false, thousands: '.', decimal: ',', affixesStay: false });


            $('.selectAdjust').each(function (index, select) {
                $('#' + select.id + ' option[value=' + select.attributes["value"].value + ']').attr('selected', 'selected');
            });
        });
    </script>

    <script type="text/javascript">
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({ pageLanguage: 'pt', includedLanguages: 'en,es,pt', layout: google.translate.TranslateElement.InlineLayout.SIMPLE, autoDisplay: false }, 'google_translate_element');
        }
    </script>
    <script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>

    <script src="~/Cork/demo3/assets/js/custom.js"></script>
    <script src="~/Cork/demo3/assets/js/scrollspyNav.js"></script>
    <script src="~/Cork/demo3/plugins/highlight/highlight.pack.js"></script>

    <!-- END GLOBAL MANDATORY SCRIPTS -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
</body>
@RenderSection("Scripts", false)
</html>
